<div class="container-fluid">
  <h1>Restaurants</h1>



    <body>
      <% @restaurants.each do |restaurant| %>
        <ul>
          <ul><%= "NAME:" %> <%= restaurant.name %></ul>
          <ul><%= link_to 'Show', restaurant %></ul>
          <ul><%= link_to 'Destroy', restaurant, method: :delete, data: { confirm: 'Are you sure?' } %></ul>
        </ul>
      <% end %>
    </body>


  <br>
  <%= link_to 'Add restaurant', new_restaurant_path %>
</div>
